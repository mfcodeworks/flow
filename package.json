{
  "name": "flow",
  "version": "0.20.200524-3",
  "author": "MF Softworks <arranjamesfletcher2012@gmail.com>",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "start:prod": "ionic serve --prod",
    "build": "ionic build --stats-json --base-href . && npm run ngsw && ionic cap copy",
    "build:prod": "ionic build --prod --aot --build-optimizer --base-href . && ionic cap copy",
    "ngsw": "npx ngsw-config www ngsw-config.json && cpx node_modules/@angular/service-worker/ngsw-worker.js www",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "android": "npm run build && cd android && gradlew assembleDebug",
    "android:prod": "npm run build:prod && cd android && gradlew assembleDebug",
    "android:run": "npm run build && ionic cap open android",
    "android:run:prod": "npm run build:prod && ionic cap open android",
    "ios": "npm run build && ionic cap open ios",
    "ios:prod": "npm run build:prod && ionic cap open ios",
    "electron": "npm run build && ionic cap open electron",
    "electron:prod": "npm run build:prod && ionic cap open electron",
    "electron:windows": "npm run build && cd electron && npm run electron:windows",
    "electron:mac": "npm run build && cd electron && npm run electron:mac",
    "electron:linux": "npm run build && cd electron && npm run electron:linux",
    "update:check": "ng update && npm outdated",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && jetifier && ionic cap update"
  },
  "private": true,
  "dependencies": {
