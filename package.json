{
  "name": "flow",
  "version": "0.41.0",
  "author": "MF Codeworks <arranjamesfletcher2012@gmail.com>",
  "homepage": "https://mfcodeworks.com/",
  "scripts": {
    "start": "ionic serve",
    "start:prod": "ionic serve --prod",
    "build": "ionic build",
    "build:small": "npm run build -- --aot --build-optimizer=true --optimization=true",
    "build:prod": "npm run build -- --prod",
    "postbuild": "npx cap copy",
    "ngsw": "npx ngsw-config www ngsw-config.json && cpx node_modules/@angular/service-worker/ngsw-worker.js www",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "res": "capacitor-resources -p android,ios",
    "android": "npm run build:small && cd android && gradlew assembleDebug",
    "android:prod": "npm run build:prod && cd android && gradlew assembleDebug",
    "android:run": "npm run build:small && npx cap open android",
    "android:run:prod": "npm run build:prod && npx cap open android",
    "ios": "npm run build:small && npx cap open ios",
    "ios:prod": "npm run build:prod && npx cap open ios",
    "electron": "npm run build:small && npx cap open electron",
    "electron:build": "npm run build:small && cd electron && npm run make",
    "electron:publish": "npm run build:prod && cd electron && npm run publish",
    "update:check": "ng update && npm outdated",
    "analyze": "ng build --stats-json",
    "postanalyze": "webpack-bundle-analyzer www/stats-es2015.json",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && jetifier && ionic cap sync"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.0-next.3",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^10.0.0-next.3",
    "@angular/core": "^10.0.0-next.3",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "^10.0.0-next.3",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^10.0.0-next.3",
    "@angular/platform-browser-dynamic": "^10.0.0-next.3",
    "@angular/router": "^10.0.0-next.3",
    "@angular/service-worker": "^10.0.0-next.3",
    "@capacitor/android": "^2.1.2",
    "@capacitor/core": "^2.1.2",
    "@capacitor/ios": "^2.1.2",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@ionic-native/barcode-scanner": "^5.26.0",
    "@ionic-native/core": "^5.26.0",
    "@ionic-native/splash-screen": "^5.26.0",
    "@ionic-native/status-bar": "^5.26.0",
    "@ionic/angular": "^5.1.1",
    "@ionic/pwa-elements": "^1.5.2",
    "big.js": "^5.2.2",
    "bootstrap": "^4.5.0",
    "core-js": "^2.5.4",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.14.0",
    "easyqrcodejs": "^3.7.0",
    "electron-push-receiver": "^2.1.3",
    "firebase": "^7.14.6",
    "localforage": "^1.7.3",
    "money": "^0.2.0",
    "phonegap-plugin-barcodescanner": "^8.1.0",
    "qr-scanner": "^1.1.1",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.7",
    "@angular-devkit/build-angular": "^0.1000.0-next.2",
    "@angular/cli": "^10.0.0-next.2",
    "@angular/compiler": "^10.0.0-next.3",
    "@angular/compiler-cli": "^10.0.0-next.3",
    "@angular/language-service": "^10.0.0-next.3",
    "@capacitor/cli": "^2.1.2",
    "@ionic/angular-toolkit": "^2.1.1",
    "@ionic/cli": "^6.9.3",
    "@types/big.js": "^4.0.5",
    "@types/crypto-js": "^3.1.47",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/localforage": "0.0.34",
    "@types/node": "^14.0.9",
    "capacitor-resources": "^2.0.3",
    "codelyzer": "^5.2.2",
    "cpx": "^1.5.0",
    "firebase-tools": "^7.16.2",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jetifier": "^1.6.5",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "protractor": "^5.4.4",
    "ts-node": "~7.0.0",
    "tslint": "^6.1.2",
    "typescript": "~3.7.5"
  },
  "browser": {
    "crypto": false
  },
  "description": "Flow Comprehensive Global Wallet"
}
